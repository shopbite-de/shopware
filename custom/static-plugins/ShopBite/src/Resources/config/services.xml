<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Veliu\ShopBite\Service\ChildrenLineItemFactory">
            <tag name="shopware.cart.line_item.factory"/>
        </service>

        <service id="Veliu\ShopBite\Core\Content\ShopBite\SalesChannel\ShopBiteRoute">
            <argument key="$systemConfigService" type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <tag>controller.service_arguments</tag>
        </service>

        <service id="Veliu\ShopBite\Service\CustomFieldsInstaller">
            <argument key="$customFieldSetRepository" type="service" id="custom_field_set.repository"/>
            <argument key="$customFieldSetRelationRepository" type="service" id="custom_field_set_relation.repository"/>
        </service>

    </services>
</container>